<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<FlowPane alignment="CENTER" styleClass="background" stylesheets="@../../assets/styles/form.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="prog.projeto.controllers.provider.StaffFormController">
    <VBox alignment="TOP_CENTER" spacing="10.0" styleClass="form-container">
        <fx:include fx:id="registerForm" source="../../widgets/register-form.fxml" />
        <HBox alignment="CENTER" prefHeight="0.0" prefWidth="0.0" spacing="8.0">
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#closeWindow" text="Cancelar" />
            <Button mnemonicParsing="false" onAction="#onRegisterSubmit" styleClass="primary" text="Submeter" />
        </HBox>
    </VBox>
</FlowPane>
